using System;

namespace ejercicio_4
{
    class Program
    {
        static void Main(string[] args)
        {
          
            Console.WriteLine("Abecedario al reves:");

            char letra= 'Z';
            letra++;

            do
            {
                letra--;
                Console.WriteLine(letra);
            } while (letra > 'A');
        }
    }
}
